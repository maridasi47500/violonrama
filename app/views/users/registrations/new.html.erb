<h2>Sign up</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "users/shared/error_messages", resource: resource %>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :password %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password" %>
  </div>

  <div class="field">
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
  </div>
  <div class="field">
    <%= f.label :country_id %><br />
    <%= f.select :country_id, Country.all.collect{|h|[h.name, h.id]} %>
  </div>
  <div class="field">
    <%= f.label :phone %><br />
    <%= f.text_field :phone%>
  </div>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name%>
  </div>
  <div class="field">
    <%= f.label :marque %><br />
    <%= f.text_field :marque%>
  </div>
  <div class="field">
    <%= f.label :annee %><br />
    <%= f.text_field :annee%>
  </div>
  <div class="field">
    <%= f.label :modele %><br />
    <%= f.text_field :modele%>
  </div>
  <div class="field">
    <%= f.label :instrumentmusique %><br />
    <%= f.text_field :instrumentmusique%>
  </div>
  <div class="field">
    <%= f.label :paysfabrique_id %><br />
    <%= f.select :paysfabrique_id, Country.all.collect{|h|[h.name, h.id]}%>
  </div>
  <div class="field">
    <%= f.label :noserie %><br />
    <%= f.text_field :noserie%>
  </div>
  <div class="field">
    <%= f.label :datearrivee %><br />
    <%= f.text_field :datearrivee%>
  </div>
  <div class="field">
    <%= f.label :city %><br />
    <%= f.text_field :city%>
  </div>



  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>

<%= render "users/shared/links" %>

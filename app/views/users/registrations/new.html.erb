<h2>Sign up</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "users/shared/error_messages", resource: resource %>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :password %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password" %>
  </div>

  <div class="field">
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
  </div>
  <div class="field">
    <%= f.label :country_id %><br />
    <%= f.select :country_id, Country.all.collect{|h|[h.name, h.id]} %>
  </div>
  <div class="field">
    <%= f.label :phone %><br />
    <%= f.text_field :phone%>
  </div>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name%>
  </div>
  <div class="field">
    <%= f.label :marque %><br />
    <%= f.text_field :marque%>
  </div>
  <div class="field">
    <%= f.label :annee %><br />
    <%= f.text_field :annee%>
  </div>
  <div class="field">
    <%= f.label :modele %><br />
    <%= f.text_field :modele%>
  </div>
  <div class="field">
<%@hey="ðŸŽ» Violin: The soul of fire is displayed on the strings
ðŸŽ¸ Viola: The soul of fire is displayed on the strings
ðŸŽ¼ Cello: The soul of fire is displayed on the strings
ðŸŽ» Double Bass: The soul of fire is displayed on the strings
ðŸŽ¹ Piano: The soul of fire is displayed on the keys
ðŸŽ¶ Harp: The soul of fire is displayed on the chords
ðŸŽ· Flute: The soul of fire is displayed in the breath
ðŸŽ¶ Oboe: The soul of fire is displayed in the reeds
ðŸŽ· Clarinet: The soul of fire is displayed in the reeds
ðŸŽ· Bassoon: The soul of fire is displayed in the reeds
ðŸŽº Trumpet: The soul of fire is displayed in the brass
ðŸŽº Trombone: The soul of fire is displayed in the brass
ðŸŽº French Horn: The soul of fire is displayed in the brass
ðŸŽº Tuba: The soul of fire is displayed in the brass
ðŸ¥ Timpani: The soul of fire is displayed on the drums
ðŸ¥ Snare Drum: The soul of fire is displayed on the drums
ðŸ¥ Bass Drum: The soul of fire is displayed on the drums
ðŸ¥ Cymbals: The soul of fire is displayed on the crash
ðŸ¥ Triangle: The soul of fire is displayed on the ring
ðŸ¥ Xylophone: The soul of fire is displayed on the wood"%>
  <h2>Choose your instrument</h2>
  
  <label for="instrument">Instrument:</label>
  <select id="instrument" name="instrument" onchange="user_instrumentmusique.value=user_instrumentmusique.value+['<%=@hey.split("\n").join("', '")%>'].filter(x => x.toLowerCase().includes(this.value))[0];">
    <option value="violin">Violin</option>
    <option value="viola">Viola</option>
    <option value="cello">Cello</option>
    <option value="doublebass">Double Bass</option>
    <option value="flute">Flute</option>
    <option value="oboe">Oboe</option>
    <option value="clarinet">Clarinet</option>
    <option value="bassoon">Bassoon</option>
    <option value="trumpet">Trumpet</option>
    <option value="trombone">Trombone</option>
    <option value="frenchhorn">French Horn</option>
    <option value="tuba">Tuba</option>
    <option value="piano">Piano</option>
    <option value="harp">Harp</option>
    <option value="timpani">Timpani</option>
    <option value="drums">Drums</option>
    <option value="cymbals">Cymbals</option>
    <option value="triangle">Triangle</option>
    <option value="xylophone">Xylophone</option>
  </select>

    <%= f.label :instrumentmusique %><br />
    <%= f.text_area :instrumentmusique%>
  </div>
  <div class="field">
    <%= f.label :paysfabrique_id %><br />
    <%= f.select :paysfabrique_id, Country.all.collect{|h|[h.name, h.id]}%>
  </div>
  <div class="field">
    <%= f.label :noserie %><br />
    <%= f.text_field :noserie%>
  </div>
  <div class="field">
    <%= f.label :datearrivee %><br />
    <%= f.text_field :datearrivee%>
  </div>
  <div class="field">
    <%= f.label :city %><br />
    <%= f.text_field :city%>
  </div>



  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>

<%= render "users/shared/links" %>
